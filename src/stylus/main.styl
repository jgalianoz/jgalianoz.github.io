body
	background:#FFF
	font-family: 'Raleway', sans-serif
	padding:0
	margin:0

body a
  transition:0.5s all
	text-decoration none

input[type="button"],input[type="submit"]
	transition:0.5s all

h1,h2,h3,h4,h5,h6
	margin:0
	padding:0

p
	margin:0
	font-family: 'Source Sans Pro', sans-serif
	letter-spacing: 1px

ul
	margin:0
	padding:0

label
	margin:0

button
	transition:0.5s all
